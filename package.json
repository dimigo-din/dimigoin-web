    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.12",
    "eslint-plugin-storybook": "^0.10.0--canary.156.ce8985b.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "readline-sync": "^1.4.10",
    "storybook": "^8.3.5",
    "typescript": "^5.6.3",
